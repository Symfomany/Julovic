
 {% extends 'SiteAdminBundle::layout.html.twig' %}


 {% block body %}
{{ parent() }}

<h1>Medias list</h1>

<table class="table" class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Offerid</th>
            <th>Restaurantid</th>
            <th>Legend</th>
            <th>Picture</th>
            <th>State</th>
            <th>Datecreated</th>
            <th>Dateupdated</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('medias_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.offerId }}</td>
            <td>{{ entity.restaurantId }}</td>
            <td>{{ entity.legend }}</td>
            <td>{{ entity.picture }}</td>
            <td>{{ entity.state }}</td>
            <td>{% if entity.dateCreated %}{{ entity.dateCreated|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{% if entity.dateUpdated %}{{ entity.dateUpdated|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('medias_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('medias_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('medias_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
 {% endblock %}