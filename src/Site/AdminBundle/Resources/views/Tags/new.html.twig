
 {% extends 'SiteAdminBundle::layout.html.twig' %}


 {% block content %}
{{ parent() }}

<h1>Tags creation</h1>

<form action="{{ path('tags_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit" class="btn btn-primary btn-large">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('tags') }}">
            Back to the list
        </a>
    </li>
</ul>

 {% endblock %}
